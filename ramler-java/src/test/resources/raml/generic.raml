#%RAML 1.0
title: Generic Type
version: 0.1
mediaType: application/json
baseUri: http://localhost:8080
annotationTypes:
  typeParam: string
  typeArgs:  string[]
  typeParams:  string[]
  notes:  string[]
types:
  Status:
    properties:
      code: integer
      httpStatus: integer
      requestId : string
      text : string
      success : boolean
  Result:
    (typeParams): [T]
    properties:
      result:
        (typeParam): T
        type: object
  Response:
    (typeParams): [T]
    properties:
      data:
        type : Result
        (typeArgs):
        - T
      status: Status
      success: boolean
  Animal:
    properties:
      species: string
      numLegs: integer
  Person:
    properties:
      firstName: string
      lastName: 
        type: string
        (notes):
        - N1
        - N2
        - N3
    example:
       {
          firstName: "Donald",
          lastName: "Duck"
       }
  AnimalResponse:
    type: Response
    (typeArgs): [Animal]
    properties:
      result:
        type: Animal
       