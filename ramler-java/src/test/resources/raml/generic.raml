#%RAML 1.0
title: Generic Type
version: 0.1
mediaType: application/json
baseUri: http://localhost:8080
uses:
  g: ../ramler.raml
annotationTypes:
  notes:  string[]
  note: string
types:
  Status:
    properties:
      code: integer
      httpStatus: integer
      requestId : string
      text : string
      success : boolean
  Result:
    (g.typeVars): [T]
    properties:
      result:
        (g.typeVar): T
        type: any
  ListResult:
    (g.typeVars):
      - T
    properties:
      result:
        (g.typeVar): T
        type: object[]
  Response: 
    (g.typeVars): [T]
    properties:
      data:
        type : Result
        (g.typeArgs):
        - T
      status: Status
      success: boolean
  Animal:
    (note): This is a note
    properties:
      species: string
      numLegs: integer
  Person:
    properties:
      firstName: string
      lastName: 
        type: string
        (notes):
        - N1
        - N2
        - N3
    example:
       {
          firstName: "Donald",
          lastName: "Duck"
       }
  AnimalResponse:
    type: Response
    (g.typeArgs): [Animal]
